<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="recyclerview3.luseen.com.recyclerview3" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="recyclerview3.luseen.com.Recyclerview.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="recyclerview3.luseen.com.Recyclerview.OnclickActivity"
            android:label="@string/title_activity_main2"
            android:parentActivityName="recyclerview3.luseen.com.Recyclerview.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="recyclerview3.luseen.com.Recyclerview.MainActivity" />
        </activity>
    </application>

</manifest>
